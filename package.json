{
  "name": "screen-o-bot",
  "author": {
    "name": "jozwik12",
    "email": "szymonjozwicki@gmail.com",
    "url": "https://github.com/jozwik12/"
  },
  "version": "1.0.0",
  "homepage": "./",
  "license": "GPL-3.0-only",
  "description": "Screen-o-bot",
  "main": "electron/electron.js",
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.1",
    "@mui/styles": "^5.4.4",
    "@reactour/tour": "^2.10.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.1",
    "python-shell": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean": "rmdir build /S /Q & rmdir dist /S /Q",
    "electron": "electron .",
    "electron-start": "node electron/electron-wait-react",
    "react-start": "react-scripts start",
    "pack": "build --dir",
    "postinstall": "install-app-deps",
    "build": "react-scripts build",
    "build-python": "robocopy python/dist/main build/main /S",
    "build-electron": "robocopy electron build/electron /S",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/electron.js --publish never",
    "dist": "yarn build && yarn build-python & yarn build-electron & yarn package"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "asarUnpack": [
      "build/main/**"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "placeholder",
      "owner": "jozwik12"
    },
    "win": {
      "icon": "./build/icon.ico",
      "target": [
        "zip"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^16.0.2",
    "electron-builder": "^22.14.5"
  }
}
